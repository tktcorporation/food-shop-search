{
  "name": "pekosearch",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "dev:all": "npm run build:client && wrangler dev",
    "build:client": "tsgo -p tsconfig.app.json --noEmit && vite build",
    "build": "tsgo -p tsconfig.app.json --noEmit && vite build",
    "preview": "vite preview",
    "lint": "oxlint --type-aware",
    "lint:quick": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsgo -p tsconfig.app.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "oxlint --type-aware && oxfmt --check . && tsgo -p tsconfig.app.json --noEmit",
    "deploy": "npm run build && wrangler deploy",
    "deploy:preview": "npm run build && wrangler deploy --branch preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply pekosearch-cache --local",
    "db:migrate:remote": "wrangler d1 migrations apply pekosearch-cache --remote"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.17",
    "hono": "^4.11.9",
    "lucide-react": "^0.564.0",
    "motion": "^12.34.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260217.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260216.1",
    "@vitejs/plugin-react": "^5.1.4",
    "drizzle-kit": "^0.31.9",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "oxlint-tsgolint": "^0.14.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
