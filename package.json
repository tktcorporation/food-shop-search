{
  "name": "gourmet-spot-search",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "dev:all": "npm run build:client && wrangler dev",
    "build:client": "tsgo -p tsconfig.app.json --noEmit && vite build",
    "build": "tsgo -p tsconfig.app.json --noEmit && vite build",
    "preview": "vite preview",
    "lint": "oxlint --type-aware",
    "lint:quick": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsgo -p tsconfig.app.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "oxlint --type-aware && oxfmt --check . && tsgo -p tsconfig.app.json --noEmit",
    "deploy": "npm run build && wrangler deploy",
    "deploy:preview": "npm run build && wrangler deploy --branch preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply gourmet-spot-cache --local",
    "db:migrate:remote": "wrangler d1 migrations apply gourmet-spot-cache --remote"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.15",
    "framer-motion": "^10.12.16",
    "hono": "^4.11.9",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript/native-preview": "^7.0.0-dev.20260126.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "drizzle-kit": "^0.31.9",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
