{
  "name": "gourmet-spot-search",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsgo -b && vite build",
    "preview": "vite preview",
    "lint": "oxlint --type-aware",
    "lint:quick": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsgo -b --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "oxlint --type-aware && oxfmt --check . && tsgo -b --noEmit",
    "deploy": "npm run build && wrangler pages deploy",
    "deploy:preview": "npm run build && wrangler pages deploy --branch preview"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.3",
    "effect": "^3.19.15",
    "framer-motion": "^10.12.16",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript/native-preview": "^7.0.0-dev.20260126.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
